<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPC\mediathequeBundle\Entity\Emprunt" table="emprunt">
    <indexes>
      <index name="ouvrage_id" columns="ouvrage_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateRetour" type="date" column="date_retour" nullable="false"/>
    <field name="dateEmprunt" type="date" column="date_emprunt" nullable="false"/>
    <many-to-one field="ouvrage" target-entity="Ouvrage" fetch="LAZY">
      <join-columns>
        <join-column name="ouvrage_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
