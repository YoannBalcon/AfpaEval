<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPC\mediathequeBundle\Entity\Utilisateurs" table="utilisateurs">
    <indexes>
      <index name="reservation_id" columns="reservation_id"/>
      <index name="emprunt_id" columns="emprunt_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="reservation" target-entity="Reservation" fetch="LAZY">
      <join-columns>
        <join-column name="reservation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="emprunt" target-entity="Emprunt" fetch="LAZY">
      <join-columns>
        <join-column name="emprunt_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
