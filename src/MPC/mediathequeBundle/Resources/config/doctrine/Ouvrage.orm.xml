<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPC\mediathequeBundle\Entity\Ouvrage" table="ouvrage">
    <indexes>
      <index name="auteur_id" columns="auteur_id"/>
      <index name="genre_id" columns="genre_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="annee" type="integer" column="annee" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <many-to-one field="auteur" target-entity="Auteurs" fetch="LAZY">
      <join-columns>
        <join-column name="auteur_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="genre" target-entity="Genre" fetch="LAZY">
      <join-columns>
        <join-column name="genre_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
